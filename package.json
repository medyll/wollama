{
	"name": "wollama",
	"version": "0.0.9",
	"author": "Medyll",
	"description": "web ui for ollama",
	"private": false,
	"type": "module",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
		}
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"release": "standard-version",
		"tauri": "tauri",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:write": "prettier --write . ",
		"link": "node link-modules.js link",
		"unlink": "node link-modules.js unlink"
	},
	"license": "MIT",
	"dependencies": {
		"@huggingface/prettier-plugin-vertical-align": "^0.2.3",
		"@medyll/idae-api": "file:../../../idae/packages/idae-api",
		"@medyll/idae-be": "file:../../../idae/packages/idae-be",
		"@medyll/idae-data-tpl": "file:../../../idae/packages/idae-data-tpl",
		"@medyll/idae-dom-events": "file:../../../idae/packages/idae-dom-events",
		"@medyll/idae-engine": "file:../../../idae/packages/idae-engine",
		"@medyll/idae-idbql": "file:../../../idae/packages/idae-idbql",
		"@medyll/idae-mongo": "file:../../../idae/packages/idae-mongo",
		"@medyll/idae-query": "file:../../../idae/packages/idae-query",
		"@medyll/idae-slotui-svelte": "file:../../../idae/packages/idae-slotui",
		"@tailwindcss/typography": "^0.5.16",
		"@tauri-apps/api": "^2.3.0",
		"@tauri-apps/plugin-shell": ">=2.2.0",
		"cheerio": "^1.0.0",
		"date-fns": "^4.1.0",
		"highlight.js": "^11.11.1",
		"hljs": "^6.2.3",
		"i18n": "^0.15.1",
		"idb": "^8.0.2",
		"jsdom": "^26.0.0",
		"marked": "^15.0.7",
		"marked-highlight": "^2.2.1",
		"ollama": "^0.5.14",
		"ollama-node": "^0.1.28",
		"prismjs": "^1.29.0",
		"ts-deepmerge": "^7.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.21.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.18.0",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.0.9",
		"@tailwindcss/vite": "^4.0.9",
		"@tauri-apps/cli": ">=2.3.1",
		"@testing-library/svelte": "^5.2.7",
		"@types/eslint": "^9.6.1",
		"@types/marked": "^6.0.0",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"mdsvex": "^0.12.3",
		"postcss": "^8.5.3",
		"postcss-import": "^16.1.0",
		"postcss-nesting": "^13.0.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.8",
		"sass": "^1.85.1",
		"sass-embedded": "^1.85.1",
		"standard-version": "^9.5.0",
		"svelte": "^5.22.1",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.9",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0",
		"vite": "^6.2.0"
	}
}
