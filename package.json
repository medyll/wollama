{
	"name": "wollama",
	"version": "0.0.9",
	"author": "Medyll",
	"description": "web ui on ollama",
	"private": false,
	"type": "module",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
		}
	},
	"main": "client/electron/main.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"release": "standard-version",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:write": "prettier --write . ",
		"link": "node link-modules.js link",
		"unlink": "node link-modules.js unlink",
		"setup:audio": "node setup-audio.js",
		"server:dev": "tsx server/server.ts",
		"test:server": "concurrently \"npm run server:dev\" \"npx tsx server/test-server.ts\" --kill-others --success first"
	},
	"license": "MIT",
	"dependencies": {
		"@capacitor/android": "^7.4.4",
		"@capacitor/app": "^7.1.0",
		"@capacitor/core": "^7.4.4",
		"@huggingface/prettier-plugin-vertical-align": "^0.2.3",
		"@tailwindcss/typography": "^0.5.19",
		"@types/multer": "^2.0.0",
		"ajv": "^8.17.1",
		"cheerio": "^1.1.2",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"express": "^5.2.1",
		"express-pouchdb": "^4.2.0",
		"highlight.js": "^11.11.1",
		"hljs": "^6.2.3",
		"i18n": "^0.15.3",
		"idb": "^8.0.3",
		"jsdom": "^27.2.0",
		"marked": "^17.0.1",
		"marked-highlight": "^2.2.3",
		"multer": "^2.0.2",
		"ollama": "^0.6.3",
		"ollama-node": "^0.1.28",
		"pouchdb": "^9.0.0",
		"pouchdb-adapter-http": "^9.0.0",
		"pouchdb-adapter-idb": "^9.0.0",
		"pouchdb-find": "^9.0.0",
		"prismjs": "^1.30.0",
		"rxdb": "^16.21.1",
		"rxjs": "^7.8.2",
		"ts-deepmerge": "^7.0.3"
	},
	"devDependencies": {
		"@capacitor/cli": "^7.4.4",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@iconify/json": "^2.2.413",
		"@iconify/svelte": "^5.1.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/svelte": "^5.2.9",
		"@types/cors": "^2.8.19",
		"@types/eslint": "^9.6.1",
		"@types/marked": "^6.0.0",
		"@types/pouchdb": "^6.4.2",
		"@types/pouchdb-find": "^7.3.3",
		"adm-zip": "^0.5.16",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"daisyui": "^5.5.5",
		"electron": "^39.2.4",
		"electron-serve": "^3.0.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"mdsvex": "^0.12.6",
		"postcss": "^8.5.6",
		"postcss-import": "^16.1.1",
		"postcss-nesting": "^13.0.2",
		"prettier": "^3.7.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.3.15",
		"sass": "^1.94.2",
		"sass-embedded": "^1.93.3",
		"standard-version": "^9.5.0",
		"svelte": "^5.45.3",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"unplugin-icons": "^22.5.0",
		"vite": "^7.2.6",
		"wait-on": "^9.0.3"
	}
}
