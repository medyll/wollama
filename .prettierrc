{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"tabWidth": 4,
	"printWidth": 130,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"filepath": "./src/**/*.{svelte,css}",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
